class Solution:
    def search(self, nums: List[int], target: int) -> int:
        l,r=0,len(nums)-1
        i=0
        while (l<r):
            #print("iteration ",i)
            m=(l+r)//2
            #print(" middle is",m)
            #print(" left is",l)
            #print(" right is",r)
            if nums[m]==target:
                return m
            if nums[r]==target:
                return r
            if nums[l]==target:
                return l
            if (nums[m]<nums[l]): 
                if nums[m]<target and target<nums[l]:
                    l=m+1
                else:
                    r=m-1 
            else: 
                if target > nums[l] and target <nums[m]:
                    r=m-1 
                else: 
                    l=m+1
        if nums[r]==target:
            return r
        return -1
