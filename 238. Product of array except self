class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        leng=len(nums)
        ans=[1 for i in range(leng)]
        pre=1
        post=1
        for i in range(1,leng):
            pre*=nums[i-1]
            post*=nums[leng-i]
            ans[i]*=pre
            ans[leng-i-1]*=post
        return ans

