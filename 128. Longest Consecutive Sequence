class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        h=set(nums)
        nums=list(h)
        ans=0
        leng=len(nums)
        for i in range(leng):
            if nums[i]-1 not in h:
                temp=1
                val=nums[i]
                while (val+1 in h):
                    temp+=1
                    val+=1
                if temp>ans:
                    ans=temp
        return ans
